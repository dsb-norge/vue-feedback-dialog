{
  "name": "@dsb-norge/vue-feedback-dialog",
  "version": "2.0.0",
  "description": "Feedback dialog based on Quasar for Vue",
  "type": "module",
  "keywords": [
    "vuejs",
    "vue.js",
    "feedback",
    "feedback dialog",
    "user feedback"
  ],
  "license": "MIT",
  "homepage": "https://github.com/dsb-norge/vue-feedback-dialog",
  "private": false,
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "preview": "vite preview",
    "version": "git add -A",
    "postversion": "git push -u --follow-tags origin master",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue-feedback-plugin.umd.js",
  "module": "./dist/vue-feedback-plugin.es.js",
  "style": "./dist/style.css",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-feedback-plugin.es.js",
      "require": "./dist/vue-feedback-plugin.umd.js"
    },
    "./dist/vue-feedback-dialog.css": "./dist/vue-feedback-dialog.css"
  },
  "peerDependencies": {
    "@quasar/extras": "^1.0.0",
    "quasar": "^2.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@dsb-norge/eslint-config-dsb-vue3-ts": "3.1.0",
    "@quasar/extras": "1.16.17",
    "@quasar/vite-plugin": "1.9.0",
    "@types/node": "22.13.8",
    "@vitejs/plugin-vue": "5.2.1",
    "quasar": "2.18.0",
    "sass": "1.85.1",
    "typescript": "5.7.3",
    "vite": "6.2.0",
    "vite-plugin-dts": "4.5.3",
    "vue": "3.5.13"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dsb-norge/vue-feedback-dialog.git"
  },
  "engines": {
    "node": "22"
  }
}
